generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  // The `url` property is no longer supported in Prisma 5+ schema files.
  // Move your connection URL to `prisma.config.ts` instead.
}
